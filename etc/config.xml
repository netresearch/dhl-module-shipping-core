<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dhlshippingsolutions>
            <version>1.1.3</version>
            <dhlglobalwebservices>
                <cut_off_time>00,00,00</cut_off_time>
                <package_dimension>[]</package_dimension>

                <shipment_defaults>
                    <terms_of_trade>DDU</terms_of_trade>
                    <export_content_type>OTHER</export_content_type>
                    <export_content_explanation>Commercial Goods</export_content_explanation>
                </shipment_defaults>
                <bulk_settings>
                    <cron_order_status>pending,processing</cron_order_status>
                </bulk_settings>
                <locationfinder_settings>
                    <maptile_api_token backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                    <maptile_url>https://api.mapbox.com/styles/v1/{id}/tiles/{z}/{x}/{y}?access_token={api_token}</maptile_url>
                    <map_copyright_attribution>www.openstreetmap.org</map_copyright_attribution>
                </locationfinder_settings>
            </dhlglobalwebservices>
        </dhlshippingsolutions>
    </default>
</config>
