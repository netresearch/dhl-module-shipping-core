<?xml version="1.0" ?>
<!-- This is a template for configurations used in the Rate processors, see Dhl\ShippingCore\Model\Config\RateConfigInterface and usages
 you can copy it into your module for setting source models to these fields -->
<include xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_include.xsd">
    <group id="shipment_settings" translate="label" sortOrder="300" showInDefault="1" showInWebsite="1" showInStore="1">
        <label><![CDATA[Shipping Preferences]]></label>
        <attribute type="expanded">1</attribute>

        <!-- Products -->
        <field id="allowedinternationalproducts" showInDefault="1" showInWebsite="1" showInStore="0" sortOrder="10"
               translate="tooltip label comment" type="Netresearch\ConfigFields\Model\Type\Checkboxset">
            <label><![CDATA[Allowed International Products]]></label>
        </field>
        <field id="alloweddomesticproducts" showInDefault="1" showInWebsite="1" showInStore="0" sortOrder="20"
               translate="tooltip label comment" type="Netresearch\ConfigFields\Model\Type\Checkboxset">
            <label><![CDATA[Allowed Domestic Products]]></label>
        </field>
    </group>
</include>
