<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminShippingSettingsConfigSection">
        <element name="shippingCoreGroup" type="button" selector="#shipping_dhlglobalwebservices-head"/>
        <element name="checkIfGroupExpanded" type="button" selector="#shipping_dhlglobalwebservices-head:not(.open)"/>
        <element name="codPaymentMethodSelection" type="multiselect" selector="#shipping_dhlglobalwebservices_cod_methods"/>
        <element name="codPaymentMethodActiveSelection" type="multiselect" selector="#shipping_dhlglobalwebservices_cod_methods option[selected=selected]"/>

        <element name="cutOffTimeConfigGroup" type="text" selector="#row_shipping_dhlglobalwebservices_cut_off_time"/>
        <element name="cutOffTimeConfigGroupHoursSelect" type="select" selector="#row_shipping_dhlglobalwebservices_cut_off_time select:nth-of-type(1)"/>
        <element name="cutOffTimeConfigGroupMinutesSelect" type="select" selector="#row_shipping_dhlglobalwebservices_cut_off_time select:nth-of-type(2)"/>
        <element name="cutOffTimeConfigGroupSecondsSelect" type="select" selector="#row_shipping_dhlglobalwebservices_cut_off_time select:nth-of-type(3)"/>

        <element name="termsOfTradeConfigGroup" type="text" selector="#row_shipping_dhlglobalwebservices_terms_of_trade"/>
        <element name="termsOfTradeConfigGroupOptionDDU" type="radio" selector="#shipping_dhlglobalwebservices_terms_of_tradeDDU"/>
        <element name="termsOfTradeConfigGroupOptionDDP" type="radio" selector="#shipping_dhlglobalwebservices_terms_of_tradeDDP"/>
    </section>
</sections>
